{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
		"name": "client",
		"image": "pkollarath/multi-client",
		"hostname": "client",
		"essential": false,
		"memory": 512
		},
		{
		"name": "server",
		"image": "pkollarath/multi-server",
		"hostname": "api",
		"essential": false,
		"memory": 512
		},
		{
		"name": "worker",
		"image": "pkollarath/multi-worker",
		"hostname": "worker",
		"essential": false,
		"memory": 512
		},
		{
		"name": "nginx",
		"image": "pkollarath/multi-nginx",
		"hostname": "nginx",
		"essential": true,
		"memoryReservation": 512,
		"portMappings": [
			{ "hostPort": 80,
			  "containerPort": 80	
			}
			],
			"links": ["client","server"]
		}
	]

}